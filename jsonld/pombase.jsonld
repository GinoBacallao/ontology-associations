{
  "name": "pombase",
  "description": "Various association data models",
  "id": "https://w3id.org/ontology_association/pombase",
  "imports": [
    "association",
    "linkml:types"
  ],
  "license": "https://creativecommons.org/publicdomain/zero/1.0/",
  "prefixes": [
    {
      "prefix_prefix": "linkml",
      "prefix_reference": "https://w3id.org/linkml/"
    },
    {
      "prefix_prefix": "biolink",
      "prefix_reference": "https://w3id.org/biolink/vocab/"
    },
    {
      "prefix_prefix": "ontology_association",
      "prefix_reference": "https://w3id.org/ontology_association/"
    },
    {
      "prefix_prefix": "RO",
      "prefix_reference": "http://purl.obolibrary.org/obo/RO_"
    },
    {
      "prefix_prefix": "FYPO",
      "prefix_reference": "http://purl.obolibrary.org/obo/FYPO_"
    },
    {
      "prefix_prefix": "owl",
      "prefix_reference": "http://www.w3.org/2002/07/owl#"
    }
  ],
  "default_curi_maps": [
    "obo_context",
    "semweb_context"
  ],
  "default_prefix": "ontology_association",
  "default_range": "string",
  "types": [
    {
      "name": "FYPO_identifier",
