  "slots": [
    {
      "name": "Database_name",
      "definition_uri": "https://w3id.org/ontology_association/Database_name",
      "description": "The database that produced the file \u2013 included to facilitate data sharing with other databases. Obviously always PomBase for this file.",
      "examples": [
        {
          "value": "PomBase",
          "@type": "Example"
        }
      ],
      "from_schema": "https://w3id.org/ontology_association/pombase",
      "is_a": "db",
      "slot_uri": "https://w3id.org/ontology_association/Database_name",
      "owner": "PombasePhafAssociation",
      "domain_of": [
        "PombasePhafAssociation"
      ],
      "range": "Database_name_enum",
      "required": true,
      "pattern": "[a-zA-Z0-9\\.\\-\\_]+",
      "@type": "SlotDefinition"
    },
    {
      "name": "Gene_systematic_ID",
      "definition_uri": "https://w3id.org/ontology_association/Gene_systematic_ID",
      "description": "The systematic ID for each gene.",
      "examples": [
        {
          "value": "SPBC1773.02c",
          "@type": "Example"
        }
      ],
      "from_schema": "https://w3id.org/ontology_association/pombase",
      "is_a": "local_id",
      "slot_uri": "https://w3id.org/ontology_association/Gene_systematic_ID",
      "owner": "PombasePhafAssociation",
      "domain_of": [
        "PombasePhafAssociation"
      ],
      "range": "string",
      "required": true,
      "pattern": "\\S+",
      "@type": "SlotDefinition"
    },
    {
      "name": "FYPO_ID",
      "definition_uri": "https://w3id.org/ontology_association/FYPO_ID",
      "description": "To use the FYPO IDs, see the [FYPO summary page](https://www.pombase.org/browse-curation/fission-yeast-phenotype-ontology) and the FAQ on browsing FYPO.",
      "examples": [
        {
          "value": "FYPO:0001164",
          "@type": "Example"
        }
      ],
      "from_schema": "https://w3id.org/ontology_association/pombase",
      "is_a": "ontology_class_ref",
      "slot_uri": "https://w3id.org/ontology_association/FYPO_ID",
      "owner": "PombasePhafAssociation",
      "domain_of": [
        "PombasePhafAssociation"
      ],
      "range": "FYPO_identifier",
      "required": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "Allele_description",
      "definition_uri": "https://w3id.org/ontology_association/Allele_description",
      "description": "The allele description specifically describes the change; see the Phenotype data bulk upload format for details on allele types and descriptions.",
      "examples": [
        {
          "value": "G126D",
          "@type": "Example"
        }
      ],
      "from_schema": "https://w3id.org/ontology_association/pombase",
      "slot_uri": "https://w3id.org/ontology_association/Allele_description",
      "owner": "PombasePhafAssociation",
      "domain_of": [
        "PombasePhafAssociation"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "Expression",
      "definition_uri": "https://w3id.org/ontology_association/Expression",
      "description": "The Expression column contains one of these values: \u2018overexpression\u2019, \u2018knockdown\u2019, \u2018endogenous\u2019, \u2018null\u2019, \u2018not specified\u2019. Deletions always have \u2018null\u2019 expression.",
      "examples": [
        {
          "value": "Overexpression",
          "@type": "Example"
        }
      ],
      "from_schema": "https://w3id.org/ontology_association/pombase",
      "slot_uri": "https://w3id.org/ontology_association/Expression",
      "owner": "PombasePhafAssociation",
      "domain_of": [
        "PombasePhafAssociation"
      ],
      "range": "Expression_enum",
      "@type": "SlotDefinition"
    },
    {
      "name": "Parental_strain",
      "definition_uri": "https://w3id.org/ontology_association/Parental_strain",
      "description": "The Parental strain column is for the parental strain designation, such as 972 h-, 975 h+, etc. This column must be filled in, but \u201cunknown\u201d is allowed.",
      "examples": [
        {
          "value": "972 h-",
          "@type": "Example"
        }
      ],
      "from_schema": "https://w3id.org/ontology_association/pombase",
      "slot_uri": "https://w3id.org/ontology_association/Parental_strain",
      "owner": "PombasePhafAssociation",
      "domain_of": [
        "PombasePhafAssociation"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "Strain_name_(background)",
      "definition_uri": "https://w3id.org/ontology_association/Strain_name_(background)",
      "description": "The Background strain name column is used for a lab\u2019s in-house name/ID/designation for the background strain (i.e. the derivative of the parental strain that has selectable marker alleles etc.). The description in the Genotype column should match this background strain.",
      "from_schema": "https://w3id.org/ontology_association/pombase",
      "slot_uri": "https://w3id.org/ontology_association/Strain_name_(background)",
      "owner": "PombasePhafAssociation",
      "domain_of": [
        "PombasePhafAssociation"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "Genotype_description",
      "definition_uri": "https://w3id.org/ontology_association/Genotype_description",
      "description": "The Background genotype description column is for alleles in the background, such as selectable markers; these details are optional. To avoid redundancy, it does not repeat the allele of interest (from column 4 or 10) in the genotype column.",
      "from_schema": "https://w3id.org/ontology_association/pombase",
      "slot_uri": "https://w3id.org/ontology_association/Genotype_description",
      "owner": "PombasePhafAssociation",
      "domain_of": [
        "PombasePhafAssociation"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "Gene_name",
      "definition_uri": "https://w3id.org/ontology_association/Gene_name",
      "description": "Gene names are optional for upload, but are included where available in the download.",
      "examples": [
        {
          "value": "bcp1",
          "@type": "Example"
        }
      ],
      "from_schema": "https://w3id.org/ontology_association/pombase",
      "slot_uri": "https://w3id.org/ontology_association/Gene_name",
      "owner": "PombasePhafAssociation",
      "domain_of": [
        "PombasePhafAssociation"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "Allele_name",
      "definition_uri": "https://w3id.org/ontology_association/Allele_name",
      "description": "Allele names are optional for upload, but are included where available in the download. Column 10 lists the preferred allele name, and any alternative names are in column 11.",
      "examples": [
        {
          "value": "bcp1+",
          "@type": "Example"
        }
      ],
      "from_schema": "https://w3id.org/ontology_association/pombase",
      "slot_uri": "https://w3id.org/ontology_association/Allele_name",
      "owner": "PombasePhafAssociation",
      "domain_of": [
        "PombasePhafAssociation"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "Allele_synonym",
      "definition_uri": "https://w3id.org/ontology_association/Allele_synonym",
      "description": "See note on Allele name above. Multiple entries are separated with pipes (|).",
      "from_schema": "https://w3id.org/ontology_association/pombase",
      "slot_uri": "https://w3id.org/ontology_association/Allele_synonym",
      "owner": "PombasePhafAssociation",
      "domain_of": [
        "PombasePhafAssociation"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "Allele_type",
      "definition_uri": "https://w3id.org/ontology_association/Allele_type",
      "description": "Allowed allele types, example descriptions, etc. are shown in the table at the bottom of the [Phenotype data bulk upload](https://www.pombase.org/documentation/phenotype-data-bulk-upload-format) format page.",
      "examples": [
        {
